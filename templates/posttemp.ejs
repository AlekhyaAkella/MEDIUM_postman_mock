<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <h1><%=post["title"]%></h1>
    </div>
</div>

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <p style="color:grey;"><%=post["desc"]%></p>
    </div>
</div>
<br>

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-1">
        <img src="index1.jpeg"></img>
    </div>
    <div class="col-md-7">
        <p>
            <font color="black"><%=post["name"]%></font>
            <button
                style="color:skyblue; border-color:skyblue; border-radius:5px; font-size:15px;">Follow</button>
        </p>
        <p style="padding-bottom: 10px;"><%=post["details"]%><span class="glyphicon glyphicon-star"
                style="margin-left:10px; padding:0;"></span>
        </p>
    </div>
</div>
</div>
